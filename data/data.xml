<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">
		<!-- Sequences for hospital patient-->
		<record id="seq_hospital_patient" model="ir.sequence">
			<field name="name">Patient Sequence</field>
			<field name="code">hospital.patient</field>
			<field name="prefix">HP</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False"/>
		</record>
		
		<!-- Sequences for hospital appointment-->
		<record id="seq_hospital_appointment" model="ir.sequence">
			<field name="name">Appointment Sequence</field>
			<field name="code">hospital.appointment</field>
			<field name="prefix">OA</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for hospital room-->
		<record id="seq_hospital_room" model="ir.sequence">
			<field name="name">Room Sequence</field>
			<field name="code">hospital.room</field>
			<field name="prefix">R</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for hospital doctor-->
		<record id="seq_hospital_doctor" model="ir.sequence">
			<field name="name">Doctor Sequence</field>
			<field name="code">hospital.doctor</field>
			<field name="prefix">D</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False"/>
		</record>

		<!-- Sequences for hospital nurse-->
		<record id="seq_hospital_nurse" model="ir.sequence">
			<field name="name">Doctor Sequence</field>
			<field name="code">hospital.nurse</field>
			<field name="prefix">N</field>
			<field name="padding">5</field>
			<field name="company_id" eval="False"/>
		</record>
	</data>
</odoo>